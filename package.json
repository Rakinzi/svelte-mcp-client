{
  "name": "svelte-mcp-client",
  "version": "0.1.0",
  "description": "Svelte MCP client with SSE transport",
  "type": "module",
  "license": "MIT",
  "author": "rakinzisilver",
  "repository": {
    "type": "git",
    "url": "https://github.com/rakinzisilver/svelte-mcp-client.git"
  },
  "bugs": {
    "url": "https://github.com/rakinzisilver/svelte-mcp-client/issues"
  },
  "homepage": "https://github.com/rakinzisilver/svelte-mcp-client#readme",
  "keywords": [
    "svelte",
    "mcp",
    "sse",
    "client"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md"
  ],
  "scripts": {
    "build": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "svelte": ">=4"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^9.0.0",
    "eslint-plugin-svelte": "^2.38.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.0",
    "svelte-eslint-parser": "^0.41.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vitest": "^1.5.0"
  }
}
